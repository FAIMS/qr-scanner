<?xml version='1.0' encoding='utf-8'?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:jr="http://openrosa.org/javarosa" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>FILL THIS IN</h:title>
    <model>
      <instance>
        <faims id="FILL_THIS_IN">
          <!--START: Style-->
          <style>
            <orientation>
              <orientation/>
            </orientation>
            <even>
              <layout_weight/>
            </even>
            <large>
              <layout_weight/>
            </large>
          </style>
          <!--END: Style-->
          <Login>
            <Login>
              <User/>
            </Login>
          </Login>
          <Control>
            <Control>
              <Scan_QR_Code/>
            </Control>
            <Search>
              <Colgroup_0>
                <Col_0>
                  <Search_Term/>
                </Col_0>
                <Col_1>
                  <Search_Button/>
                </Col_1>
              </Colgroup_0>
              <Entity_List/>
            </Search>
          </Control>
          <Record>
            <Record>
              <ID/>
              <Data/>
            </Record>
          </Record>
        </faims>
      </instance>
    </model>
  </h:head>
  <h:body>
    <!--START: Style-->
    <group ref="style">
      <label/>
      <group ref="orientation">
        <label/>
        <input ref="orientation">
          <label>horizontal</label>
        </input>
      </group>
      <group ref="even">
        <label/>
        <input ref="layout_weight">
          <label>1</label>
        </input>
      </group>
      <group ref="large">
        <label/>
        <input ref="layout_weight">
          <label>3</label>
        </input>
      </group>
    </group>
    <!--END: Style-->
    <group ref="Login">
      <label>{Login}</label>
      <group ref="Login" faims_scrollable="false">
        <label>{Login}</label>
        <select1 appearance="compact" ref="User">
          <label>{User}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
      </group>
    </group>
    <group ref="Control">
      <label>{Control}</label>
      <group ref="Control">
        <label>{Control}</label>
        <trigger ref="Scan_QR_Code">
          <label>{Scan_QR_Code}</label>
        </trigger>
      </group>
      <group ref="Search" faims_scrollable="false">
        <label>{Search}</label>
        <group ref="Colgroup_0" faims_style="orientation">
          <label/>
          <group ref="Col_0" faims_style="even">
            <label/>
            <input ref="Search_Term">
              <label>{Search_Term}</label>
            </input>
          </group>
          <group ref="Col_1" faims_style="large">
            <label/>
            <trigger ref="Search_Button">
              <label>{Search_Button}</label>
            </trigger>
          </group>
        </group>
        <select1 appearance="compact" ref="Entity_List">
          <label>{Entity_List}</label>
          <item>
            <label>Options not loaded</label>
            <value>0</value>
          </item>
        </select1>
      </group>
    </group>
    <group ref="Record" faims_archent_type="Record">
      <label>{Record}</label>
      <group ref="Record">
        <label>{Record}</label>
        <input faims_attribute_name="ID" faims_attribute_type="measure" ref="ID" faims_read_only="true">
          <label>{ID}</label>
        </input>
        <input faims_attribute_name="Data" faims_attribute_type="measure" ref="Data">
          <label>{Data}</label>
        </input>
      </group>
    </group>
  </h:body>
</h:html>
